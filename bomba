def royxat():
    for i in r:
        print(i)
sanoq = 5
while sanoq!=0:
    royxat()
    q = int(input('Qatorni kiriting: '))
    u = int(input('Ustunni kiriting: '))
    if q==qa and u==us:
        r[q-1][u-1]='{ðŸ’£}'
        royxat()
        print('Bombaga tushdiz!!')
        break
    else:
        r[q - 1][u - 1] = '{ðŸ¥³}'
        sanoq -= 1
    if sanoq == 0:
        print('Tabriklaymiz, Siz yutdingiz')
        r[q - 1][u - 1] = '{ðŸ¥³}'
        royxat()
